{
    "name": "simian",
    "version": "1.5.2",
    "description": "Flexible JS monkey-patching simple enough for monkey brains!",
    "author": "Yellowsink",
    "repository": "github:yellowsink/simian",
    "license": "BSD-3-Clause",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "devDependencies": {
        "esbuild": "^0.14.8",
        "typescript": "^4.5.4"
    },
    "scripts": {
        "postinstall": "rm -rf dist",
        "prepublishOnly": "rm -rf dist && tsc && esbuild src/*.ts --format=cjs --outdir=dist/cjs"
    },
    "keywords": [
        "javascript",
        "js",
        "mod",
        "patcher",
        "patch",
        "monkey-patcher",
        "monkey-patch",
        "injector",
        "injection",
        "guerrilla-patcher",
        "guerrilla-injector"
    ]
}
